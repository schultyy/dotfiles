[user]
  useconfigonly = true
	name = Jan Schulte
	email = janschulte@fastmail.com
[push]
  default = simple
[core]
  editor = vim
	excludesfile = ~/.gitignore
	autocrlf = input
[alias]
  co = checkout
  ci = commit
  st = status
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
[color]
  ui = true
[init]
	defaultBranch = main
[pull]
	rebase = false
